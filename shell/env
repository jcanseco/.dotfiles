#!/usr/bin/env bash

source ~/.dotfiles/shell/helpers

append_to_path ~/.dotfiles/shell/bin
append_to_path ~/.fzf/bin # fzf binary

# Golang
export GOPATH=~/Workspace/go
prepend_to_path $GOPATH/bin
# Use Go distribution installed at /usr/local/go and 'go' binary at
# /usr/local/go/bin. Note: this is where install_golang.sh installs to.
export GOROOT=/usr/local/go
prepend_to_path $GOROOT/bin
